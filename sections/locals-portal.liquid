<script>
  const rc_locals = {
  {% for local in section.blocks %}

    {% if local.settings.title contains " - " %}
    "{{ local.settings.title | split: " - " | last }}": {
      
      {% for valueLocals in local.settings %}
      
      {% if valueLocals == "title" %}
        {% continue %}
      {% else %}
        {{ valueLocals }}: "{{ local.settings[valueLocals] }}",    
      {% endif %}
      
      {% endfor %}

    },
    {% endif %}

  {% endfor %}
  }

  const rc_selectorLanguage = [
    {% for local in section.blocks %}
      {% if local.settings.title contains " - " %}

      {
        language: "{{ local.settings.title | split: " - " | first }}",
        code: "{{ local.settings.title | split: " - " | last }}",
      },

      {% endif %}
    {% endfor %}
  ]

</script>

