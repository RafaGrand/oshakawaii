{% comment %}
  ly_global_begin
{% endcomment %}
{% capture ly-global-content %}
  {% render 'ly-global' render:true %}
{% endcapture %}
{% assign ly-global-content = ly-global-content | split: '#ly#' %}
{% assign langify_title = ly-global-content[0] %}
{% assign langify_description = ly-global-content[1] %}
{% assign langify_shop_name = ly-global-content[2] %}
{% assign langify_shop_description = ly-global-content[3] %}
{% assign canonical_langify_url = ly-global-content[4] %}
{% assign langify_shop_url = ly-global-content[5] %}
{% assign langify_shop_domain = ly-global-content[6] %}
{% comment %}
  ly_global_end
{% endcomment %}
<ul class="pagination-list">
  {% unless paginate.previous.is_link %}
    <li aria-hidden="true">
      <button disabled>
        <
      </button>
    </li>
    {% else %}
    <li>
      <a href="{{ paginate.previous.url }}">
        <
      </a>
    </li>
  {% endunless %}
  {% for part in paginate.parts %}
    <li>{% comment %}ly_code_replace_for_[ part.title | link_to: part.url]_begin{% endcomment %}{% capture ly_translation %}{% render 'ly-title' with part render:true %}{% endcapture %}{{ ly_translation | link_to: part.url}}{% comment %}ly_code_replace_end{% endcomment %}</li>
  {% endfor %}
  {% unless paginate.next.is_link %}
    <li aria-hidden="true">
      <button disabled>
        >
      </button>
    </li>
  {% else %}
    <li>
      <a href="{{ paginate.next.url }}">
        >
      </a>
    </li>
  {% endunless %}
</ul>
